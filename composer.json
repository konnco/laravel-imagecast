{
    "name": "konnco/laravel-imagecast",
    "description": "Make easier to store images into database",
    "keywords": [
        "konnco",
        "laravel",
        "laravel-imagecast"
    ],
    "homepage": "https://github.com/konnco/laravel-imagecast",
    "license": "MIT",
    "authors": [
        {
            "name": "Franky So",
            "email": "frankyso.mail@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^9.0",
        "illuminate/contracts": "^8.37",
        "intervention/image": "^2.5",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Konnco\\ImageCast\\": "src",
            "Konnco\\ImageCast\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konnco\\ImageCast\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Konnco\\ImageCast\\ImageCastServiceProvider"
            ],
            "aliases": {
                "ImageCast": "Konnco\\ImageCast\\ImageCastFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
